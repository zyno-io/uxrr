{
    "name": "@zyno-io/uxrr-api",
    "version": "0.1.0",
    "license": "SEE LICENSE IN LICENSE",
    "private": true,
    "main": "dist/src/bootstrap.js",
    "scripts": {
        "clean": "dksf-dev clean",
        "build": "dksf-dev build",
        "dev": "dksf-dev run",
        "start": "node . server:start",
        "migrate": "dksf-dev migrate",
        "test": "dksf-dev test $(find tests -name '*.spec.ts' -not -path '*/integration/*' | sort)",
        "test:int": "UXRR_DEV_MODE=true dksf-dev test tests/integration/",
        "test:all": "dksf-dev test",
        "format": "npx oxlint --fix --fix-suggestions --fix-dangerously && prettier --config ../../.prettierrc.json --ignore-path ../../.gitignore --write ."
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.990.0",
        "@aws-sdk/lib-storage": "^3.990.0",
        "@deepkit/postgres": "1.0.19",
        "@zyno-io/dk-server-foundation": "26.216.907",
        "ioredis": "^5.9.3",
        "jose": "^6.1.3",
        "mime-types": "^3.0.2"
    },
    "devDependencies": {
        "@types/lodash": "^4.17.23",
        "@types/mime-types": "^3.0.1",
        "@types/node": "^25.2.3",
        "patch-package": "^8.0.1",
        "tslib": "^2.8.1",
        "typescript": "~5.9"
    }
}
